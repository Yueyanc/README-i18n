{
  "name": "rdi18n",
  "type": "module",
  "version": "0.1.0",
  "license": "MIT",
  "homepage": "https://github.com/Yueyanc/README-i18n",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yueyanc/README-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/Yueyanc/README-i18n/issues"
  },
  "bin": {
    "rdi18n": "./bin/rdi18n"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup src/index.ts --watch --onSuccess \"node ./bin/rdi18n translate -k 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 --host 127.0.0.1 -p 10809\"",
    "build": "tsup src/index.ts",
    "release": "bumpp && npm publish"
  },
  "author": "Yueyanc",
  "dependencies": {
    "chalk": "^5.3.0",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "openai": "^4.24.7",
    "tunnel": "^0.0.6",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/listr": "^0.14.9",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.5",
    "@types/tunnel": "^0.0.6",
    "@types/yargs": "^17.0.32",
    "bumpp": "^9.3.0",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
